         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><div subtype="section" n="a" type="textpart"><ab>
                  <lb n="1"/><w lemma="bibo">Bibe</w> <persName type="attested"><name type="cognomen" nymRef="Issicuar">Issicua<lb n="2" type="inWord"/>r</name></persName> <w lemma="quia">quia</w> 
                  <w lemma="mereo">mer<lb n="3" type="inWord"/>eris</w> <w lemma="cum">cum</w> 
                  <w lemma="filius">fi<lb n="4" type="inWord"/>li</w> 
