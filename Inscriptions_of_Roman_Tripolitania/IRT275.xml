               <lb n="8"/><w lemma="flamen">flaminis</w> <persName type="emperor"><w lemma="diuus">diui</w> <name type="cognomen" nymRef="Vespasianus">Vespas<supplied reason="lost">iani</supplied></name></persName>
               <gap reason="lost" atLeast="8" atMost="9" unit="character"/>
