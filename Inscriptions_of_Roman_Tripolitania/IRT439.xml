               <lb n="4"/><name type="cognomen" nymRef="Seuerus">Seueri</name> <name type="cognomen" nymRef="Pius">Pii</name> <name type="cognomen" nymRef="Pertinax">Pertina<lb n="5" type="inWord"/>cis</name> 
               <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><ex>usti</ex></expan></name> <name>Arabici</name> 
               <name type="cognomen" nymRef="Adiabenicus">A<lb n="6" type="inWord"/>diabenici</name> <name type="cognomen" nymRef="Parthicus">Parthi<lb n="7" type="inWord"/>ci</name> 
               <w lemma="magnus">maximi</w></persName> 
               <w lemma="fortis">fortissi<lb n="8" type="inWord"/>mi</w> <w lemma="felix">felicissimi</w>
               <lb n="9"/><w lemma="pater"><expan><abbr>p</abbr><ex>atris</ex></expan></w> 
               <w lemma="patria"><expan><abbr>p</abbr><ex>atriae</ex></expan></w> <w lemma="filius">filio</w> <w lemma="et">et</w> 
               <w lemma="imperator"><expan><abbr>Imp</abbr><ex>eratoris</ex></expan></w>
               <lb n="10"/><persName type="emperor"><name type="cognomen" nymRef="Caesar"><expan><abbr>Caes</abbr><ex>aris</ex></expan></name> 
