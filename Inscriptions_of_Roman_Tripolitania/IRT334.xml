                     <persName type="emperor"><name type="praenomen" nymRef="Tiberius"><supplied reason="lost"><expan><abbr>Ti</abbr><ex>beri</ex></expan></supplied></name>
                  <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><ex>usti</ex></expan></name></persName> 
                  <w lemma="filius"><expan><abbr>f</abbr><ex>ilio</ex></expan></w></persName> 
                  <w lemma="pontifex"><expan><abbr>pont</abbr><ex>ifici</ex></expan></w>  
                  <w lemma="augur"><supplied reason="lost"><expan><abbr>augur</abbr><ex>i</ex></expan></supplied></w> 
