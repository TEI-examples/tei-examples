         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><ab>
               <lb n="1"/><persName type="emperor"><name type="gentilicium" nymRef="Iulia">Iuliae</name> <name type="cognomen" nymRef="Augusta">Augustae</name>
               <lb n="2"/><w lemma="mater">matris</w> <w lemma="castra">castrorum</w> 
               <lb n="3"/><w lemma="mater">matri</w> 
                  <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><supplied reason="lost"><del rend="erasure"><ex>ustorum</ex></del></supplied></expan></name></persName>
               <w lemma="coniunx">coniugi</w> 
               <lb n="4"/><w lemma="inuictus">inuicti</w>
               <lb n="5"/>
               <persName type="emperor"><w lemma="imperator"><expan><abbr>Imp</abbr><ex>eratoris</ex></expan></w> 
               <name type="cognomen" nymRef="Caesar"><expan><abbr>Caes</abbr><ex>aris</ex></expan></name> 
               <name type="praenomen" nymRef="Lucius"><expan><abbr>L</abbr><ex>uci</ex></expan></name> <name type="gentilicium" nymRef="Septimius">Septimi</name>
               <lb n="6"/><name type="cognomen" nymRef="Seuerus">Seueri</name> <name type="cognomen" nymRef="Pius">Pii </name> <name type="cognomen" nymRef="Pertinax">Pertinacis</name>
               <lb n="7"/>
               <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><ex>usti</ex></expan></name> 
               <name><expan><abbr>Arab</abbr><ex>ici</ex></expan></name> <name type="cognomen" nymRef="Adiabenicus">Adiabenici</name>
               <lb n="8"/><name type="cognomen" nymRef="Parthicus">Parthici</name> <w lemma="magnus">maximi</w></persName>
               <lb n="9"/><persName type="attested"><name type="praenomen" nymRef="Marcus"><expan><abbr>M</abbr><ex>arcus</ex></expan></name> <name type="gentilicium" nymRef="Iunius">Iunius</name> <name type="cognomen" nymRef="Punicus">Punicus</name></persName>  
               <lb n="10"/><w lemma="procurator"><expan><abbr>proc</abbr><ex>orator</ex></expan></w> <w lemma="sexagenarius">sexagenarius</w>
               <lb n="11"/><w lemma="prouincia">prouinciae</w> <placeName type="ethnic" nymRef="Thracia">Thraciae</placeName>
