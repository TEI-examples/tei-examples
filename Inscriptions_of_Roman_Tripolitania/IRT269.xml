               <w lemma="patria">pat<supplied reason="lost">ria</supplied>e</w> 
                  <persName type="attested"><name type="cognomen" nymRef="Annobal">Annobalis</name> <name type="cognomen" nymRef="Ruso">Rusonis</name></persName>
               </persName> 
