               <lb n="2"/>
               <persName type="attested"><name type="praenomen" nymRef="Caius"><expan><abbr>C</abbr><ex>aius</ex></expan></name> <name type="gentilicium" nymRef="Rubellius">Rubellius</name> <name type="cognomen" nymRef="Blandus">Blandus</name></persName> 
