               <lb n="5"/><w lemma="se">sibi</w> <w lemma="ob">ob</w> <w lemma="mereo">merita</w> 
               <w lemma="decerno">decr<supplied reason="lost">e</supplied><lb n="6" type="inWord"/>tam</w> 
               <w lemma="suus"><expan><abbr>s</abbr><ex>ua</ex></expan></w> 
               <w lemma="pecunia"><expan><abbr>p</abbr><ex>ecunia</ex></expan></w> 
               <w lemma="facio"><expan><abbr>f</abbr><ex>ecit</ex></expan></w>
