               <persName type="attested"><name type="praenomen" nymRef="Marcus"><expan><abbr>M</abbr><ex>arcus</ex></expan></name> <name type="gentilicium" nymRef="Vlpius">Vlpius</name><gap reason="lost" extent="unknown" unit="character"/>
               <lb n="2"/><name type="cognomen" nymRef="Chinitiu-"><seg part="I">Chinitiu</seg></name></persName><gap reason="lost" extent="unknown" unit="character"><certainty match=".." locus="name"/></gap>
               
               <supplied reason="lost"><w lemma="se">sibi</w> <w lemma="et">et</w></supplied>
