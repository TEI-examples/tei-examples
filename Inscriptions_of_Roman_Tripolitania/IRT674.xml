         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><ab>
               <lb n="1"/><w lemma="facio">Fecit</w> <w lemma="et">et</w> <w lemma="pono">posuit</w> 
               <persName type="attested"><name type="gentilicium" nymRef="Caecilia">Cae<lb n="2" type="inWord"/>cilia</name> <name type="cognomen" nymRef="Namgyddi">Namgyddi</name></persName> 
               <w lemma="filius">fi<lb n="3" type="inWord"/>lio</w> <w lemma="suus">suo</w> <persName type="attested"><name type="cognomen" nymRef="Africanus">Africano</name> 
