               <w lemma="femina"><expan><abbr>f</abbr><ex>emina</ex></expan></w> <w lemma="filia">filia</w> <w lemma="et">et</w>
               <lb n="10"/><w lemma="heres">heres</w> <w lemma="pono">posuit</w> 
