         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><ab>
               <lb n="1"/>
               <persName type="emperor"><w lemma="imperator"><expan><abbr>Imp</abbr><ex>eratore</ex></expan></w> 
               <name type="cognomen" nymRef="Caesar"><expan><abbr>Caes</abbr><ex>are</ex></expan></name> 
               <name type="praenomen" nymRef="Titus"><expan><abbr>T</abbr><ex>ito</ex></expan></name> 
               <name type="gentilicium" nymRef="Aelius">A<supplied reason="lost">elio</supplied></name> <name type="cognomen" nymRef="Hadrianus"><supplied reason="lost">Hadriano</supplied></name> <name type="gentilicium" nymRef="Antoninus">Antonino</name>
               <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><ex>usto</ex></expan></name> <name type="cognomen" nymRef="Pius">Pio</name></persName> 
               <w lemma="pontifex"><expan><abbr>pont</abbr><ex>ifice</ex></expan></w> 
               <w lemma="magnus"><expan><abbr>max</abbr><ex>imo</ex></expan></w> 
