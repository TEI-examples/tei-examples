               <lb n="3"/><name type="cognomen" nymRef="Hadrianus">Hadriano</name> <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><ex>usto</ex></expan></name></persName> 
               <w lemma="pontifex"><expan><abbr>pont</abbr><ex>ifice</ex></expan></w> 
