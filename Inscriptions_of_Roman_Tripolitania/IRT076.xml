         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><ab>
               <lb n="1"/><persName type="emperor"><w lemma="imperator"><expan><abbr>Imp</abbr><ex>eratori</ex></expan></w> <name type="cognomen" nymRef="Caesar"><expan><abbr>C</abbr><supplied reason="lost"><abbr>aes</abbr><ex>ari</ex></supplied></expan></name>
               <gap reason="lost" extent="unknown" unit="character"/>
               <lb n="2"/><w lemma="pontifex"><expan><abbr>po</abbr><supplied reason="lost"><abbr>nt</abbr><ex>ifici</ex></supplied></expan></w> 
               <w lemma="magnus"><supplied reason="lost"><expan><abbr>max</abbr><ex>imo</ex></expan></supplied></w>
               <gap reason="lost" extent="unknown" unit="character"/>
                  <lb n="3"/><name type="cognomen" nymRef="Persicus">Persico</name> <w lemma="magnus">m<supplied reason="lost">aximo</supplied></w></persName> <gap reason="lost" extent="unknown" unit="character"/>
               <lb n="4"/><w lemma="pater">pater</w> <gap reason="lost" extent="unknown" unit="character"/>
            </ab></div>
         <div type="apparatus">
