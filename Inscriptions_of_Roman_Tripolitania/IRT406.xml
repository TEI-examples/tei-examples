         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><ab>
              <lb n="1"/><persName type="emperor"><name type="gentilicium" nymRef="Iulia">Iulia<supplied reason="lost">e</supplied> </name><name type="cognomen" nymRef="Augusta">Augustae</name></persName>
                 <lb n="2"/><w lemma="mater">matri</w> <w lemma="castra">castrorum</w>
               <lb n="3"/><w lemma="mater">matri</w> 
