               <lb n="1"/><persName type="emperor"><w lemma="imperator"><expan><abbr>Im<unclear>p</unclear></abbr><ex>erator</ex></expan></w> <name type="cognomen" nymRef="Caesar"><supplied reason="lost">Caesar</supplied></name>
               <lb n="2"/><name type="cognomen" nymRef="Domitianus">Domitia<supplied reason="lost">nus</supplied></name> <name type="cognomen" nymRef="Augustus"><expan><abbr>Aug</abbr><ex>ustus</ex></expan></name>
