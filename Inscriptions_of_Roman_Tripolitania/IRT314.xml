               <lb n="3"/><w lemma="dono">donui</w> <w lemma="cura">cura</w> <persName type="attested"><name type="praenomen" nymRef="Lucius"><expan><abbr>L</abbr><ex>uci</ex></expan></name> <name type="gentilicium" nymRef="Tettius">Tetti</name> <name type="cognomen" nymRef="Comus">Comi</name></persName> 
               <w lemma="libertus"><expan><abbr>I</abbr><ex>iberti</ex></expan></w> 
