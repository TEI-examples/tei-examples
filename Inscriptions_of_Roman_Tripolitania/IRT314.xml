         <div type="edition" xml:lang="la" xml:space="preserve"><head xml:lang="en">Text</head><ab>
               <lb n="1"/><persName type="attested"><name type="praenomen" nymRef="Lucius"><expan><abbr>L</abbr><ex>ucius</ex></expan></name> <name type="gentilicium" nymRef="Tettius">Tettius</name> <name type="cognomen" nymRef="Eutychus">Eutychus</name></persName> <w lemma="in">in</w> <w lemma="suus">suo</w> <w lemma="suus">sua</w>
               <lb n="2"/><w lemma="pecunia"><expan><abbr>pecun</abbr><ex>ia</ex></expan></w> <w lemma="cisterna"><expan><abbr>cistern</abbr><ex>am</ex></expan></w> <persName type="divine"><name nymRef="Venus">Veneri</name></persName> <w lemma="et">et</w> 
