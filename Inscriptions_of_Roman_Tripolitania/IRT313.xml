               <lb n="7"/><w lemma="σύν">σὺν</w> <w lemma="ὁ">τῇ</w> <w lemma="θυγάτηρ">θυγα<lb n="8" type="inWord"/>τρὶ</w> <persName type="attested"><name type="cognomen" nymRef="Πρωτάρη">Πρωτάρ<lb n="9" type="inWord"/>ῃ</name></persName>
